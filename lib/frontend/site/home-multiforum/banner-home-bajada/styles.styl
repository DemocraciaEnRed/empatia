#banner-home-bajada
  padding: 0;
  .jumbotron-body
    text-align: center
    padding: 25vh 0;
    // gradient background from purple to cyan
    background: linear-gradient(to bottom, #7b4397dd 0%, #dc243055 100%);
  .bajada-image
    width: 100%
    max-width: 400px
    margin: 0 auto
    display: block
    margin-bottom: 20px
  .bajada-text
    color: #FFF
    font-weight: 800;
    text-transform: uppercase;
    font-size: 5rem;
  .bajada-logo
    margin-bottom: 20px;